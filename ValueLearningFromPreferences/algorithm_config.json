"ff": {
        "name": "FireFighters-v0",
        "n_values": 2,
        "values_names": ["Proximity", "Professionalism"],
        "values_short_names": ["Prox", "Prof"],

        "K": 1,
        "L": 4,
        "horizon": 50,

        "assume_variable_horizon": false,
        "is_contextual": false,
        
        "basic_profiles": [[1.0, 0.0], [0.0,1.0]],
        "profiles_colors": [[1,0,0], [0,0,1]],
